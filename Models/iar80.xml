<!-- CANOPY -->
	<animation>
		<type>translate</type>
		<object-name>Canopy</object-name>
		<object-name>CanopyRel</object-name>
		<object-name>RvMirror</object-name>
		<object-name>GlassR</object-name>
		<object-name>GlassL</object-name>
		<object-name>GlassInR</object-name>
		<object-name>GlassInL</object-name>
		<property>controls/canopy/position-norm</property>
		<factor>0.55</factor>
		<axis>
			<x> 1.000</x>
			<y> 0.000</y>
			<z>-0.055</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>CanopyRel</object-name>
		<property>controls/canopy/position-norm</property>
		<interpolation>
			<entry>		<ind>0.0</ind>	<dep>  0</dep>	</entry>
			<entry>		<ind>0.1</ind>	<dep>-30</dep>	</entry>
			<entry>		<ind>0.2</ind>	<dep>-50</dep>	</entry>
			<entry>		<ind>1.0</ind>	<dep>-50</dep>	</entry>
		</interpolation>
		<axis>
			<x1-m>4.418</x1-m>	<y1-m>0.289</y1-m>	<z1-m>0.457</z1-m>
			<x2-m>4.434</x2-m>	<y2-m>0.289</y2-m>	<z2-m>0.456</z2-m>
		</axis>
	</animation>

	<animation>
		<type>pick</type>
		<object-name>CanopyRel</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>property-toggle</command>
				<property>controls/canopy/opened</property>
			</binding>
		</action>
	</animation>

