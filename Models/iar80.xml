<!-- COWL FLAPS -->
	<animation>
		<type>select</type>
		<object-name>CowlFlapsO</object-name>
		<condition>
			<greater-than>
				<property>controls/engines/engine[0]/cowl-flaps-norm</property>
				<value>0.8</value>
			</greater-than>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>CowlFlaps4</object-name>
		<condition>
			<and>
				<less-than-equals>
					<property>controls/engines/engine[0]/cowl-flaps-norm</property>
					<value>0.8</value>
				</less-than-equals>
				<greater-than>
					<property>controls/engines/engine[0]/cowl-flaps-norm</property>
					<value>0.6</value>
				</greater-than>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>CowlFlaps3</object-name>
		<condition>
			<and>
				<less-than-equals>
					<property>controls/engines/engine[0]/cowl-flaps-norm</property>
					<value>0.6</value>
				</less-than-equals>
				<greater-than>
					<property>controls/engines/engine[0]/cowl-flaps-norm</property>
					<value>0.4</value>
				</greater-than>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>CowlFlaps2</object-name>
		<condition>
			<and>
				<less-than-equals>
					<property>controls/engines/engine[0]/cowl-flaps-norm</property>
					<value>0.4</value>
				</less-than-equals>
				<greater-than>
					<property>controls/engines/engine[0]/cowl-flaps-norm</property>
					<value>0.2</value>
				</greater-than>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>CowlFlaps1</object-name>
		<condition>
			<and>
				<less-than-equals>
					<property>controls/engines/engine[0]/cowl-flaps-norm</property>
					<value>0.2</value>
				</less-than-equals>
				<greater-than>
					<property>controls/engines/engine[0]/cowl-flaps-norm</property>
					<value>0.1</value>
				</greater-than>
			</and>
		</condition>
	</animation>

	<animation>
		<type>select</type>
