<!--propeller-->
	<animation>
		<type>select</type>
		<enable-hot>false</enable-hot>
		<object-name>Propeller</object-name>
		<condition>
			<and>
				<less-than>
					<property>engines/engine/rpm</property>
					<value>600.001</value>
				</less-than>
				<not>
				    <property>sim/failure-manager/propeller/bent</property>
				</not>
			</and>
		</condition>
	</animation>

	<animation>
		<type>spin</type>
		<enable-hot>false</enable-hot>
		<object-name>Propeller</object-name>
		<property>engines/engine/rpm</property>
		<factor>0.66</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>select</type>
		<enable-hot>false</enable-hot>
		<object-name>PropBlur</object-name>
		<condition>
			<and>
				<greater-than>
					<property>engines/engine[0]/rpm</property>
					<value>600.000</value>
				</greater-than>
				<less-than>
					<property>engines/engine[0]/rpm</property>
					<value>1500.001</value>
				</less-than>
				<not>
				    <property>sim/failure-manager/propeller/bent</property>
				</not>
			</and>
		</condition>
	</animation>

	<animation>
		<type>spin</type>
		<enable-hot>false</enable-hot>
		<object-name>PropBlur</object-name>
		<property>engines/engine/rpm</property>
		<factor>0.66</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>select</type>
		<enable-hot>false</enable-hot>
		<object-name>PropDisc</object-name>
		<condition>
			<and>
				<greater-than>
					<property>engines/engine[0]/rpm</property>
					<value>1500.000</value>
				</greater-than>
				<not>
				    <property>sim/failure-manager/propeller/bent</property>
				</not>
			</and>
		</condition>
	</animation>

	<animation>
		<type>spin</type>
		<enable-hot>false</enable-hot>
		<condition>
			<greater-than>
				<property>engines/engine[0]/rpm</property>
				<value>1500.000</value>
			</greater-than>
		</condition>
		<object-name>PropDisc</object-name>
		<property>engines/engine/rpm</property>
		<factor>0.008</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

